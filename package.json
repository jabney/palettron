  "homepage": "https://github.com/jabney/palettron#readme",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "colord": "^2.9.2",
    "seedable": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@types/tap": "^15.0.7",
    "tap": "^16.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
